# ============================================================================
# SEEDANCE VIDEO GENERATION (fal.ai) - REQUIRED
# ============================================================================

# fal.ai API Key for Seedance 1.5 Pro
# Get your key from: https://fal.ai/dashboard
FAL_KEY=your_fal_api_key_here

# Seedance video resolution (optional, defaults to 720p)
# Options: 720p (only supported option currently)
SEEDANCE_RESOLUTION=720p

# ============================================================================
# ELEVENLABS SPEECH-TO-SPEECH ENHANCEMENT - OPTIONAL
# ============================================================================

# Enable STS audio enhancement (default: false)
# Applies only to audio_mode="raw" videos
# Enhances audio quality of the final concatenated video
ENABLE_STS_ENHANCER=false

# ElevenLabs voice ID for STS conversion
# Use a consistent, high-quality voice for all enhanced videos
# Find voices at: https://elevenlabs.io/voice-library
ELEVENLABS_STS_VOICE_ID=your_voice_id_here

# STS Model ID (optional, defaults to eleven_multilingual_sts_v2)
# Current model: eleven_multilingual_sts_v2 (best for multilingual)
ELEVENLABS_STS_MODEL_ID=eleven_multilingual_sts_v2

# Note: ELEVENLABS_API_KEY is already required for TTS and will be reused for STS

# ============================================================================
# EXISTING VARIABLES (NO CHANGES REQUIRED)
# ============================================================================

# Worker Configuration
MAX_CONCURRENT_JOBS=1
JOB_DELAY_MS=10000
ENABLE_TTS=true

# Higgsfield (Keyframe Generation)
HIGGSFIELD_API_KEY=your_higgsfield_key
HIGGSFIELD_API_KEY_SECRET=your_higgsfield_secret
HIGGSFIELD_BASE_IMAGE_URL=https://example.com/base_image.png

# ElevenLabs (TTS)
ELEVENLABS_API_KEY=your_elevenlabs_key
ELEVENLABS_DEFAULT_VOICE_EN=your_default_en_voice_id
ELEVENLABS_DEFAULT_VOICE_ES=your_default_es_voice_id
ELEVENLABS_DEFAULT_VOICE_EN_MALE=optional_male_voice_id
ELEVENLABS_DEFAULT_VOICE_EN_FEMALE=optional_female_voice_id
ELEVENLABS_DEFAULT_VOICE_ES_MALE=optional_male_voice_id
ELEVENLABS_DEFAULT_VOICE_ES_FEMALE=optional_female_voice_id

# Google Cloud (Legacy - Veo not used but config still loaded)
GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id
GOOGLE_CLOUD_REGION=us-central1
VERTEX_VIDEO_MODEL=veo-3.1-fast-generate-preview
VERTEX_IMAGE_MODEL=imagen-4.0-ultra-generate-001

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# ============================================================================
# MIGRATION CHECKLIST
# ============================================================================
# 1. Add FAL_KEY to your environment
# 2. Keep ENABLE_STS_ENHANCER=false initially
# 3. Test with 1-scene jobs in both audio modes
# 4. Enable STS gradually after validation
# 5. Monitor costs and quality metrics
